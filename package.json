{
  "name": "true-badge-web",
  "version": "1.0.0",
  "description": "\"Express server for TrueBadge\"",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@shelf/jest-mongodb": "^1.1.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "dev": "start nodemon",
    "start": "nodemon --exec babel-node index.js --presets @babel/preset-env",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "./test/.*test.js$",
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "setupFiles": [
      "./test/setup.js"
    ],
    "preset": "@shelf/jest-mongodb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michielu/true-badge-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Michielu/true-badge-web/issues"
  },
  "homepage": "https://github.com/Michielu/true-badge-web#readme"
}

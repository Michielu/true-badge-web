{
  "name": "true-badge-web",
  "version": "1.0.0",
  "description": "\"Express server for TrueBadge\"",
  "main": "src/index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@shelf/jest-mongodb": "^1.1.5",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel src --out-dir ./build/src  --source-maps",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/src/index.js",
    "dev": "nodemon src/index.js --exec babel-node",
    "prod": "npm run build && node ./build/src/index.js",
    "test": "jest",
    "test -debug": "jest --detectOpenHandles"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "./test/.*test.js$",
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "setupFiles": [
      "./test/setup.js"
    ],
    "preset": "@shelf/jest-mongodb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michielu/true-badge-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Michielu/true-badge-web/issues"
  },
  "homepage": "https://github.com/Michielu/true-badge-web#readme"
}